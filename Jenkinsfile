pipeline {
    agent any
    
    stages {
        stage('greetings') {
            steps{
                echo 'Holla Mundos!'
            }
        }
        stage('death') {
            steps {
                echo 'I am very very dead'
            }
        }
        stage('Preparation') {
            steps {
                echo 'Preparation'
            }
        }
        stage('Build') {
            steps {
                echo 'Build'
            }
        }
        stage('Results') {
            steps {
                echo 'Results'
            }
        }
    }
}
